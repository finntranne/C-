#include <bits/stdc++.h>

using namespace std;

bool prime(int a)
{
    if (a < 2)
        return false;
    for (int i = 2; i <= sqrt(a); i++)
        if (a%i==0)
            return false;
    return true;
}

int minPrime(vector<int> A, int n, int i = 0, int min = INT_MAX)
{
    if (i == n)
        return min;
    if (prime(A[i]) == true && A[i] < min)
        min = A[i];
    return
        minPrime(A,n,i+1,min);
}

void solve()
{
    int n;
    cin >> n;
    vector<int> A(n);
    for (int i = 0; i < n; i++)
        cin >> A[i];
    cout << minPrime(A,n) << endl;
}

int main()
{
    // nhap so test case
    int t;
    cin >> t;
    while (t--)
        solve();
    return 0;
}
